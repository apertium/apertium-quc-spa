<?xml version="1.0" encoding="UTF-8"?>
<rules>
<!--ixim ulew = tierra dura-->

  <rule>
<!--ukʼojol ri kʼaslik Dyos-->
    <match lemma="kʼojolaxel" tags="n.*"> <select lemma="hijo" tags="n.*"/> </match>
    <match/> <match lemma="kʼaslik"/>
  </rule>
  <rule>
    <match lemma="kʼaslik" tags="adj"> <select lemma="viviente"/> </match>
    <match lemma="dyos"/>
  </rule>

</rules>
